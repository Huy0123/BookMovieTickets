    "express": "^4.21.0",
    "express-handlebars": "^8.0.1",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "fs": "^0.0.1-security",
    "google-auth-library": "^9.14.1",
